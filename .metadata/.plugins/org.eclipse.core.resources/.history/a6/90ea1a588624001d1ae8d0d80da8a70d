#include <stdio.h>
#include <stdlib.h>
#define NUM 33

int main(void) {

	setbuf(stdout, NULL);

	int a;
	int b;
	int resultdo;

	a = 1;
	b = 1;

	resultdo = a && b;

	printf("El reultado de a interseccion b es: %d", resultdo);

	return EXIT_SUCCESS;
}
